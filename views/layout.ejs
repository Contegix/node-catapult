<% if (this.session.email) { %>
<nav class="nav clearfix">
    <div class="nav-meta">
        <ul>
            <li><a href="/" style="font-weight: bold;"><%= this.session.name %></a></li>
            <li><a href="https://wiki.opower.com/x/Epe8">Help</a></li>
            <li><a href="/logout"">Sign out</a></li>
        </ul>
    </div>
    <a href="/" class="logo"><img src="/images/logo.png" alt="Candidate Catapult" width="275" height="90" /></a>
    <div class="tabs">
        <div class="tabgroup">
        <%- partial('nav') %>
        </div>
    </div>
</nav>
<div class="content">
<% } %>
<%- body %>
<% if (this.session.email) { %>

</div>
<footer class="footer">
    <div class="keys clearfix">


        <div class="key">
            <div>Last data grab from Jobvite: <strong><%= last_updated() ? Util.minutes(last_updated()) + ' minutes ago' : 'Unknown' %></strong>.</div>
            <div>Next update: <strong><%= last_updated() ? Util.minutes(+last_updated() + (Constants.UPDATE_INTERVAL_MINUTES * Constants.MILLISECONDS_PER_MINUTE)) * -1 + ' minutes' : 'Unknown' %></strong>.</div>
            <div>Data is updated every 45 minutes, or you can ask Dylan for an immediate update.</div>
            <% if (in_progress()) { %><div><strong>Data is updating <em>right now!</em></strong></div><% } %>
        </div>


        <% if (locals.has_ticks) { %>
        <div class="key clearfix">
            <span class="tick low">0 &ndash; 10 Days</span>
            <span class="tick medium">11  &ndash; 30 Days</span>
            <span class="tick high"> 30 Days or More </span>
            <div>Each dot represents a candidate who has been in that particular workflow step for the above number of days.</div>
        </div>
        <% } %>

        <% if (locals.has_mini) { %>
        <div class="key clearfix">
                <div class="candidate-mini">
                    <div class="tag color3">
                        <span class="days">5</span>
                    </div>
                    <div class="mini-content color3">
                        <div class="name employeeReferral">
                            Candidate 1
                        </div>
                    </div>
                </div>

                <div class="candidate-mini employeeReferral">
                    <div class="tag color3">
                        <span class="days">16</span>
                    </div>
                    <div class="mini-content color3">
                        <div class="name employeeReferral">
                            Candidate 2
                        </div>
                    </div>
                </div>

            <div><strong>Number:</strong>  How many days the candidate has been in that workflow step.</div>
            <div><strong>Bold Names:</strong> Candidate was referred by an OPOWER employee.</div>
            <div><strong>Colors:</strong> Each color is a different open req.</div>
        </div>
        <% } %>

    </div>

    <div class="confidential">OPOWER CONFIDENTIAL</div>
</footer>
<% } %>

<script type="text/javascript" src="/js/jquery/jquery.js"></script>
<script type="text/javascript" src="/js/jquery/jquery.doTimeout.js"></script>
<script type="text/javascript" src="/js/jquery/jquery.isotope.js"></script>
<script type="text/javascript" src="/js/jquery/jquery.store.js"></script>
<script type="text/javascript" src="/js/jquery/jquery.flextarea.js"></script>
<script type="text/javascript" src="/js/jqueryui/js/jquery-ui-1.8.9.custom.js"></script>
<script type="text/javascript" src="/js/datatables/jquery.dataTables.js"></script>
<script type="text/javascript" src="/js/datatables/FixedHeader.js"></script>
<script type="text/javascript" src="/js/datatables/ColReorder.js"></script>
<script type="text/javascript" src="/js/catapult.js"></script>
<script type="text/javascript" src="/js/search.js"></script>
<script type="text/javascript" src="/js/tables.js"></script>
<script type="text/javascript" src="/js/filtering.js"></script>
<script type="text/javascript" src="/js/candidate.js"></script>
<script type="text/javascript" src="/js/req.js"></script>


